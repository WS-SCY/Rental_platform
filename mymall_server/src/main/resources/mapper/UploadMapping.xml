<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mymall.dao.UploadDao">
    <insert id="UploadInfo">
        insert into house (distric,storey,area,price,homeownerid,rentedflag,sharflag,brifintroduction,gender,icon,introduction  )
        values(#{distric},#{storey},#{area},#{price},#{homeownerid},0,#{shareflag},#{brifintroduction},#{gender},#{icon},#{introduction})
    </insert>
</mapper>